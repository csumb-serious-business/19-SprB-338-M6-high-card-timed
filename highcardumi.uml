          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_aYyR4mGsEemTNvAnnLvP1w"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_aYyR42GsEemTNvAnnLvP1w" value=""/>
